{
    "name": "64robots/stripe",
    "description": "A package to add stripe integration 64 robots packages",
    "keywords": [
        "64Robots",
        "R64",
        "stripe"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "6.*",
        "fzaninotto/faker": "^1.4",
        "stripe/stripe-php": "^6.4",
        "carbon/carbon": "^1.*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "R64\\Stripe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R64\\Stripe\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "R64\\Stripe\\StripeServiceProvider"
            ]
        }
    }
}